{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "ui/**/*",
    "component/**/*",
    "images/**/*",
    "data/**/*",
    "resources/**/*",
    "fonts/**/*",
    "sound/**/*",
    "manifest.json",
    "service_worker.js",
    "index.html",
    "404.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "client",
      "addPushManifest": true,
      "insertPrefetchLinks": true,
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": {
        "inlineCss": true,
        "inlineScripts": true,
        "stripComments": true
      }
    }
  ],
  "lint": {
    "warningsToIgnore": []
  },
  "moduleResolution": "node",
  "npm": true
}